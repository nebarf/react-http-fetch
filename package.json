    "test": "jest -c ./jest.config.js",
    "test:watch": "jest -c ./jest.config.js --watch"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "dependencies": {
    "axios": "^0.22.0"
  }
