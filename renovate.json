{
  "extends": [
    "config:base",
    ":semanticCommits"
  ],
  "baseBranches": ["main"],
  "prHourlyLimit": 2,
  "timezone": "Europe/Rome",
  "lockFileMaintenance": {
    "enabled": true
  },
  "rangeStrategy": "update-lockfile",
  "schedule": ["after 10pm every weekday", "before 4am every weekday", "every weekend"]
}
